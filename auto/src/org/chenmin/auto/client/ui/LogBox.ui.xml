<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel>
	<g:HorizontalPanel>
		<g:Button ui:field="hideButton" text="关闭日志" />
		<g:Button ui:field="showButton" text="显示日志" />
	</g:HorizontalPanel>
	<g:ScrollPanel ui:field="scroll" height="170px" width="400px">
		<g:HTML ui:field="log"></g:HTML>
	</g:ScrollPanel>
	</g:VerticalPanel>
</ui:UiBinder> 